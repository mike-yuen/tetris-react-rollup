{
  "name": "tetris-react-rollup",
  "version": "1.0.13",
  "description": "A simple tetris game with React and Rollup",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "npm run clean:style-inject",
    "build:local": "npm run build && npm pack",
    "build:publish": "npm run build && npm publish",
    "clean:style-inject": "node scripts/replaceStyleInjectPath.js"
  },
  "keywords": [
    "\"Tetris\"",
    "\"React\"",
    "\"Rollup\""
  ],
  "author": "mikeyuen",
  "license": "MIT",
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-storysource": "7.1.1",
    "@storybook/addons": "7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-webpack5": "^7.1.1",
    "@storybook/theming": "7.1.1",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "css-loader": "6.8.1",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "file-loader": "6.2.0",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-redux": "^8.1.2",
    "rollup": "3.27.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.64.1",
    "sass-loader": "13.3.2",
    "storybook": "^7.1.1",
    "style-loader": "3.3.3",
    "ts-loader": "9.4.4",
    "tslib": "2.6.1",
    "typescript": "5.1.6",
    "webpack": "5.88.2"
  },
  "peerDependencies": {
    "tslib": "^2.1.0"
  }
}
