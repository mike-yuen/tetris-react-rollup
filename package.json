{
  "name": "tetris-react-rollup",
  "version": "1.0.6",
  "description": "A simple tetris game with React and Rollup",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "npm run clean:style-inject",
    "build:local": "npm run build && npm pack",
    "build:publish": "npm run build && npm publish",
    "clean:style-inject": "node scripts/replaceStyleInjectPath.js"
  },
  "keywords": [
    "\"Tetris\"",
    "\"React\"",
    "\"Rollup\""
  ],
  "author": "mikeyuen",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "19.0.1",
    "@rollup/plugin-node-resolve": "13.0.2",
    "@rollup/plugin-typescript": "8.2.3",
    "@storybook/addon-essentials": "6.4.0-alpha.20",
    "@storybook/addon-storysource": "6.4.0-alpha.20",
    "@storybook/addons": "6.4.0-alpha.20",
    "@storybook/builder-webpack5": "6.4.0-alpha.20",
    "@storybook/manager-webpack5": "6.4.0-alpha.20",
    "@storybook/react": "6.4.0-alpha.20",
    "@storybook/theming": "6.4.0-alpha.20",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "css-loader": "6.2.0",
    "eslint": "7.31.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "postcss": "8.3.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.53.3",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "style-loader": "1.3.0",
    "ts-loader": "9.2.4",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "webpack": "5.46.0"
  },
  "dependencies": {
    "style-inject": "^0.3.0"
  },
  "peerDependencies": {
    "tslib": "^2.1.0"
  }
}
